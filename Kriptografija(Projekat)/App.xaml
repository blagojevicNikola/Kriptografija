<Application x:Class="Kriptografija_Projekat_.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Kriptografija_Projekat_"
             xmlns:vms="clr-namespace:Kriptografija_Projekat_.ViewModel"
             xmlns:views="clr-namespace:Kriptografija_Projekat_.View">
    <Application.Resources>
        <DataTemplate DataType="{x:Type vms:CheckSertificateViewModel}">
            <views:CheckSertificateView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type vms:LoginViewModel}">
            <views:LoginView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type vms:RegisterViewModel}">
            <views:RegisterView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type vms:FilesOverviewViewModel}">
            <views:FilesOverviewView/>
        </DataTemplate>
    </Application.Resources>
</Application>
